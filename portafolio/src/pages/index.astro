---
import Layout from '../layouts/Layout.astro';
import Badge from '../components/Badge.astro'
import LinkedInIcon from '../components/icons/LinkedIn.astro'
import SocialPill from '../components/SocialPill.astro'
import GithubIcon from '../components/icons/GithubIcon.astro'
import Experience from '../components/Experience.astro'
import SectionContainer from '../components/SectionContainer.astro'
import Briefcase from '../components/Briefcase.astro'
import Code from '../components/icons/Code.astro'
---

<Layout title="Astro Porfolio">
	<main class="px-5">
		<SectionContainer class="py-44 pb-20">
			<img class="rounded-full w-12 h-12 mb-5" 
    			src="data:image/jpeg;base64," 
    			alt="lui"
			/>
			<h1 class="text-white text-3xl md:text-4xl lg:text-5xl font-bold flex flex-row gap-x-4 pb-4">
				Hey, soy Lui 
				<a
				href="https://www.linkedin.com/in/luis-ramos-a149082a8/" 
				target="_blank"
				rel="noopener"
				class="flex justify-center items-center">
					<Badge>Junior Onboarding Developer</Badge>
				</a>
			</h1>
			<h2 class="text-xl opacity-80 text-wrap">
				+1 año de experiencia en Database Administration y Backend Development.
				Entusiasta de la administraciòn de servidores, Despliegue de Aplicaciones, 
				Arquitectura en la nube, Cloud Digital Leader y DevOps Specialist.
			</h2>
				<nav class="flex gap-x-2 mt-8">
					<SocialPill href="https://www.linkedin.com/in/luis-ramos-a149082a8/" >
						<LinkedInIcon class="w-6 h-6" />
						LinkedIn
					</SocialPill>
					<SocialPill href="https://github.com/NoCap940">
						<GithubIcon class="w-6 h-6" />
						Github
					</SocialPill>
				</nav>
		</SectionContainer>
			

		<SectionContainer id="experiencia">
			<h2 class="text-2xl font-semibold mb-6 flex justify-center gap-x-2 items-center">
				<Briefcase class="size-6"/>
				Proffesional Experience
			</h2>
			<Experience />
		</SectionContainer>

		<SectionContainer id="proyectos">
			<h2 class="text-2xl font-semibold mb-6 flex justify-center gap-x-2 items-center">
				<Code class="size-6"/>
				Proyectos
			</h2>
			<Experience />
		</SectionContainer>
	</main>
</Layout>

